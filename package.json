{
  "name": "battleofbrooklyn-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@capacitor/android": "^8.0.1",
    "@capacitor/core": "^8.0.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^13.5.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
  "start": "react-scripts start",
  "build": "react-scripts build",
  "build:dev": "env-cmd -f .env.development react-scripts build",
  "build:emulator": "env-cmd -f .env.emulator react-scripts build",
  "build:production": "env-cmd -f .env.production react-scripts build",
  "test": "react-scripts test",
  
 
  "cap:sync": "npx cap sync",
  "cap:sync:dev": "npm run build:dev && npx cap sync",
  "cap:sync:emulator": "npm run build:emulator && npx cap sync",
  "cap:sync:production": "npm run build:production && npx cap sync",
  

  "android:emulator": "npm run build:emulator && npx cap sync && npx cap run android",
  
 
  "android:studio": "npx cap open android",
  
  
  "android:build": "npm run build:production && npx cap sync && npx cap open android"
},
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@capacitor/cli": "^8.0.1",
    "env-cmd": "^10.1.0"
  }
}